<main>
  <div class="container">
    <div>
      <% if (products.length> 0) { %>
        <div class='products-grid'>
          <% for( const product of products ) { %>
            <article class='product'>
              <img class='product-image'
                src="data:image/jpeg;base64,"
                alt="product image">
              <div class="product-content">
                <h2 class='product-title'>
                  <%= product.title %>
                </h2>
                <p class='product-price'>$ 12.99</p>
                <p class='product-description'>Lorem ipsum dolor sit amet consectetur adipisicing elit. Velit adipisci
                  quaerat praesentium harum.</p>
                <button class='btn'>Add To Cart</button>
              </div>
            </article>
            <% } %>
        </div>

        <% } else { %>
          <p class='not-found-message'>No product found!</p>
          <% } %>
    </div>
  </div>
</main>